{
  "extends": "tslint:latest",
  "rules": {
    "indent": [true,
        "spaces"
    ],
    "quotemark": [true,
      "single",
      "avoid-escape"
    ],
    "max-line-length": [true, 80],
    "no-consecutive-blank-lines": [true, 2],
    "only-arrow-functions": false,
    "no-console": [false],
    "object-literal-sort-keys": false,
    "object-literal-shorthand": false,
    "arrow-parens": false,
    "variable-name": false,
    "no-unused-variable": true
  }
}
