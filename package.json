{
  "name": "scout-service",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "//": "=> ETC",
    "start": "npm run build && npm run server",
    "start-dev": "npm run build && npm run babel-and-server-live",
    "build": "(npm run lint && npm run compile-ts && npm run compile-babel) || true",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint -c ./tslint.json './src/**/*.ts' || true",
    "fix-lint": "tslint -c ./tslint.json --fix './src/**/*.ts'",
    "compile-ts": "tsc",
    "compile-babel": "webpack -d",
    "compile-babel-live": "webpack -d --watch",
    "server": "node build/final/service.js",
    "server-live": "nodemon --inspect build/final/service.js --watch build/final",
    "babel-and-server-live": "concurrently --kill-others \"npm run server-live\" \"npm run compile-babel-live\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "mongodb": "^2.2.21",
    "mongoose": "^4.7.6",
    "typescript": "^2.1.5"
  },
  "devDependencies": {
    "@types/mongodb": "^2.1.37",
    "@types/mongoose": "^4.7.3",
    "@types/node": "^7.0.0",
    "@types/whatwg-fetch": "0.0.33",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.11.0",
    "ts-loader": "^1.3.3",
    "tslint-eslint-rules": "^3.2.3",
    "webpack": "^1.14.0"
  }
}
